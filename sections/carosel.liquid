<div class="container">
    <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner" style="width:100%; max-height: 500px !important;">
            <div class="carousel-item active">
                <img class="d-block w-100" src="{{ section.settings.upload_image | img_url: " master", crop: 'center'
                    }}" alt="">
            </div>
            <div class="carousel-item ">
                <img class="d-block w-100" src="{{ section.settings.upload_image_2 | img_url: " master", crop: 'center'
                    }}" alt="">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls"
            data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls"
            data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</div>

{% schema %}
{
"name": "Carosel",
"settings": [
{
"type": "image_picker",
"id": "upload_image",
"label": "Upload Banner Image"
},
{
"type": "image_picker",
"id": "upload_image_2",
"label": "Upload Banner Image"
}
]
}
{% endschema %}