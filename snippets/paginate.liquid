{% if paginate.pages > 0 %}
<nav aria-label="Page navigation example">
    <ul class="pagination">
        {% if paginate.previous %}
        <li class="page-item"><a class="page-link" href="{{ paginate.previous.url }}">{{ paginate.previous.title
                }}</a>
        </li>
        {% endif %}
        {% for part in paginate.parts %}
        {% if part.is_link %}
        <li class="page-item "><a class="page-link" href="{{ part.url }}">{{ part.title }}</a></li>
        {% else %}
        <li class="page-item active"><span class="page-link">{{ part.title }}</span></li>
        {% endif %}
        {% endfor %}
        {% if paginate.next %}
        <li class="page-item"><a class="page-link" href="{{ paginate.next.url }}">{{ paginate.next.title }}</a></li>
        {% endif %}
        <!-- <li class="page-item"><a class="page-link" href="#">1</a></li>
                <li class="page-item"><a class="page-link" href="#">2</a></li>
                <li class="page-item"><a class="page-link" href="#">3</a></li>
                <li class="page-item"><a class="page-link" href="#">Next</a></li> -->
    </ul>
</nav>
{% endif %}